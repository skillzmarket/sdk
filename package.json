{
  "name": "@skillzmarket/sdk",
  "version": "0.1.0",
  "description": "SDK for discovering and calling paid AI skills with automatic x402 payments",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "prepublishOnly": "npm run build && npm run test"
  },
  "keywords": [
    "ai",
    "skills",
    "marketplace",
    "x402",
    "crypto",
    "payments",
    "usdc",
    "base",
    "ethereum",
    "sdk"
  ],
  "author": "Skillz Market",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/skillzmarket/sdk.git"
  },
  "homepage": "https://skillzmarket.com",
  "bugs": {
    "url": "https://github.com/skillzmarket/sdk/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@x402/fetch": "^2.2.0",
    "@x402/evm": "^2.2.0",
    "viem": "^2.0.0"
  },
  "peerDependencies": {
    "viem": "^2.0.0"
  },
  "devDependencies": {
    "typescript": "^5.0.0",
    "@types/node": "^20.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
